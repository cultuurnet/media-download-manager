{
    "name": "cultuurnet/media-download-manager",
    "description": "Download Media to different platforms",
    "type": "project",
    "license": "Apache-2.0",
    "authors": [],
    "require": {
        "silex/silex": "~1.3",
        "symfony/form": "~2.4",
        "symfony/validator": "~2.4",
        "symfony/config": "~2.4",
        "symfony/security-csrf": "~2.4",
        "symfony/twig-bridge": "~2.4",
        "twig/twig": "^1.23",
        "twitter/bootstrap": "^3.3.6",
        "deralex/yaml-config-service-provider": "~1.0",
        "knplabs/console-service-provider": "~1.0",
        "guzzle/guzzle": "^2.8 || ^3.0",
        "monolog/monolog": "~1.11",
        "cultuurnet/valueobjects": "~3.0",
        "league/flysystem": "~1.0",
        "srmklive/flysystem-dropbox-v2": "1.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "twistor/flysystem-http": "~0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "escapestudios/symfony2-coding-standard": "~2.9",
        "squizlabs/php_codesniffer": "~2.8.1",
        "phing/phing": "~2.14",
        "satooshi/php-coveralls": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "CultuurNet\\MediaDownloadManager\\": ["src/", "app/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CultuurNet\\MediaDownloadManager\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
